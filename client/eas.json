{
  "build": {
    "preview": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug",
        "autoIncrement": true
      },
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "EXPO_PUBLIC_FORCE_ASSET_INCLUSION": "true"
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle",
        "autoIncrement": true
      },
      "channel": "production",
      "distribution": "store"
    }
  },
  "cli": {
    "version": ">= 12.3.0",
    "appVersionSource": "remote"
  },
  "submit": {
    "production": {}
  }
}
